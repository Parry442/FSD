{
  "name": "application-testing-suite",
  "version": "1.0.0",
  "description": "Comprehensive testing management platform with AI-driven automation",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "migrate": "node server/database/migrate.js",
    "seed": "node server/database/seed.js",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "testing",
    "automation",
    "ai",
    "test-management",
    "quality-assurance"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "sequelize": "^6.35.0",
    "multer": "^1.4.5-lts.1",
    "xlsx": "^0.18.5",
    "openai": "^4.20.1",
    "socket.io": "^4.7.4",
    "nodemailer": "^6.9.7",
    "joi": "^17.11.0",
    "uuid": "^9.0.1",
    "moment": "^2.29.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  }
}