{
  "name": "application-testing-suite",
  "version": "1.0.0",
  "description": "Cloud-native AI-powered testing suite with AWS integration",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "cd client && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "migrate": "node server/database/migrate.js",
    "seed": "node server/database/seed.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "docker:build": "docker build -t testing-suite .",
    "docker:run": "docker run -p 5000:5000 -p 3000:3000 testing-suite"
  },
  "keywords": [
    "testing",
    "automation",
    "ai",
    "ml",
    "data-quality",
    "api-testing",
    "ui-testing",
    "cloud-native"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "uuid": "^9.0.1",
    "joi": "^17.11.0",
    "winston": "^3.11.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "sequelize": "^6.35.2",
    "sqlite3": "^5.1.6",
    "xlsx": "^0.18.5",
    "socket.io": "^4.7.4",
    "axios": "^1.6.2",
    "faker": "^6.6.6",
    "chance": "^1.1.11"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "prettier": "^3.1.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "nock": "^13.4.0",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.2",
    "ts-node": "^10.9.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/testing-suite.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/testing-suite/issues"
  },
  "homepage": "https://github.com/yourusername/testing-suite#readme"
}